generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model brawlers {
  id           Int     @id @default(autoincrement())
  name         String  @db.VarChar(50)
  rarity       String  @db.VarChar(50)
  wallbreaker  Boolean
  base_health  Int
  release_year Int
  image_url    String? @db.VarChar(255)
}
